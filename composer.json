{
    "name": "kduma/emszmal-api",
    "type": "library",
    "description": "emSzmal Banking API wrapper in PHP",
    "keywords": [
        "KDuma",
        "emSzmalAPI"
    ],
    "homepage": "https://github.com/kduma/L5-emSzmal-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Krystian Duma",
            "email": "open-source@kduma.tk",
            "homepage": "http://kduma.tk/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "guzzlehttp/guzzle": "^6.2",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "KDuma\\emSzmalAPI\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "KDuma\\emSzmalAPI\\Laravel\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
